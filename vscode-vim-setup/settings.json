    "editorCursor.foreground": "#ff0000",
    "[Abyss]": {
      "editor.selectionBackground": "#0d335a",
      "statusBar.background": "#170038",
      "statusBar.foreground": "#b6b5b5",
      "statusBar.border": "#232C34",
      "activityBar.activeBackground": "#280858",
      "tab.activeBackground": "#280858",
      "tab.activeBorder": "#9900ff",
      "sideBar.border": "#232C34",
      "tab.hoverBorder": "#420099",
      "editorLineNumber.foreground": "#3e505a",
      "sideBarSectionHeader.background": "#051729",
      "scrollbarSlider.background": "#160a2e"
    }
  },
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "python.languageServer": "Pylance",
  "[json]": {
    "editor.quickSuggestions": {
      "strings": "on"
    },
    "editor.suggest.insertMode": "replace",
    "editor.defaultFormatter": "esbenp.prettier-vscode"
