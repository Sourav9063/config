{
  "editor.tokenColorCustomizations": {
    "[Abyss]": {
      "textMateRules": [
        {
          "name": "Function argument",
          "scope": "variable.parameter",
          "settings": {
            "fontStyle": "",
            "foreground": "#6688CC"
          }
        },
        {
          "name": "Class name",
          "scope": [
            "entity.name.class",
            "entity.name.type",
            "entity.name.namespace",
            "entity.name.scope-resolution"
          ],
          "settings": {
            "fontStyle": "italic",
            "foreground": "#9966b8"
          }
        }
      ]
    }
  },
  "git.ignoreMissingGitWarning": true,
  "editor.wordWrapColumn": 40,
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off"
  },
  "editor.renderControlCharacters": false,
  "workbench.colorCustomizations": {
    "editor.background": "#000000",
    "terminal.background": "#000",
    "editorBracketHighlight.foreground1": "#750de6",
    "editorBracketHighlight.foreground2": "#376cff",
    "editorBracketHighlight.foreground3": "#31e9e3",
    "editorBracketHighlight.foreground4": "#0e9a0e",
    "editorBracketHighlight.foreground5": "#f1f51c",
    "editorBracketHighlight.foreground6": "#fa9231",
    "editorBracketMatch.background": "#7c00e2a6",
